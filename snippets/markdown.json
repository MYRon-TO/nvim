{
    "todo": {
        "prefix": [ "todo" ],
        "body": [
            "<-- !todo: ${1:discription} -->$2"
        ],
        "discription": "todo",
        "scope": "markdown"
    },
    "CodeBlock": {
        "prefix": [ "C" ],
        "body": [
            "```$1",
            "```$2"
        ],
        "discription": "Code Block",
        "scope": "markdown"
    }
}
